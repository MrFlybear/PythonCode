    "def histogram(s):\n",
    "    d=dict()\n",
    "    for c in s:\n",
    "        if c not in d:\n",
    "            d[c]=1\n",
    "        else:\n",
    "            d[c]=d[c]+1\n",
    "    return d\n",
    "s='xiongxiang ai cai cai'\n",
    "h=histogram(s)\n",
    "def print_hist(h):\n",
    "    for c in sorted(h):\n",
    "        print(c,h[c])\n",
    "print_hist(h)\n",
