   "source": [
    "import re\n",
    "\n",
    "def has_no_e(s):\n",
    "    return re.search(r'[eE]',s)==None\n",
    "\n",
    "fin=open('C:\\word.txt')\n",
    "sum1=0\n",
    "num=0\n",
    "for line in fin:\n",
    "    sum1=sum1+1\n",
    "    word=line.strip()\n",
    "    if(has_no_e(word)):\n",
    "        num=num+1\n",
    "percent=num/sum1\n",
    "print(percent)\n",
    "\n",
